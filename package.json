{
  "name": "jupyter-chatbot",
  "publisher": "ethanloi",
  "license": "MIT",
  "repository": {
  "type": "git",
  "url": "https://github.com/LoiLoi-10/jupyter-chatbot.git"
  },
  "displayName": "jupyter-chatbot",
  "description": "RAG for Jupyter Notebook",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0" 
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:jupyter-notebook"
  ],
  "extensionDependencies": [
    "ms-python.python"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jupyter-chatbot.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "jupyter-chatbot.openChat",
        "title": "Open Chatbot",
        "icon": {
          "light": "icons/jupyter-chatbot-icon.png",
          "dark": "icons/jupyter-chatbot-icon.png"
        }
      }
    ],
    "notebooks": [
      {
        "type": "jupyter-chatbot",
        "displayName": "Jupyter Notebook",
        "selector": [
          {
            "filenamePattern": "*.ipynb"
          }
        ]
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jupyter-chatbot",
          "title": "Jupyter Chatbot",
          "icon": "icons/jupyter-chatbot-icon.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "webpack",
    "watch": "webpack --watch",
    "build": "webpack --mode production --devtool hidden-source-map",
    "package": "npm run build && vsce package --out dist/",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run build && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.4",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^2.15.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^9.25.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "axios": "^1.10.0"
  }
}